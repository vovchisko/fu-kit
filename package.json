{
  "name": "fu-kit",
  "version": "0.2.0",
  "description": "",
  "main": "dist/fu-kit.ssr.js",
  "browser": "dist/fu-kit.esm.js",
  "module": "dist/fu-kit.esm.js",
  "unpkg": "dist/fu-kit.min.js",
  "files": [
    "docs/*",
    "dist/*",
    "src/**/*.vue",
    "src/**/*.scss",
    "reset.scss",
    "root.scss",
    "scss.scss"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve src/docs/main.js",
    "build:docs": "vue-cli-service build src/docs/main.js --dest docs",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config rollup.config.js --format iife"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.0.11",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "postcss": "^8.2.10",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue": "^3.0.5"
  },
  "peerDependencies": {
    "vue": "^3.0.5",
    "vue-router": "^4.0.0-0"
  },
  "engines": {
    "node": ">=12"
  }
}
